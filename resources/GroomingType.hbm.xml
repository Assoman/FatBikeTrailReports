<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.paulawaite.fbtr.entity.GroomingType" table="grooming_type"
           schema="FAT_BIKE_TRAIL_REPORTS">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="grooming">
            <column name="grooming" sql-type="varchar(30)" length="30"
                    not-null="true"/>
        </property>
    </class>
</hibernate-mapping>